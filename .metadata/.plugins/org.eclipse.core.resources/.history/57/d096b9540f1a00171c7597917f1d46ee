
public class test {

	public static void changeStr(String str) {
		str = "abc";
		str = new String("abc");
	}
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub

		
		String str1 = "ABC";
		String str2 = str1;
		System.out.println(str2);
		changeStr(str1);
		System.out.println(str1);
	}

}
