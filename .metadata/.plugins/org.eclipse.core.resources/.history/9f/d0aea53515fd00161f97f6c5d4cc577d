package Amz;

public class NthPrime {
	
	public static int solution(int n)
	{
		if (n <= 0 ) return 0;
		
		int num = 1;
		
		while(n>0)
		{
			num++;
			int i;
			for (i=2; i<=num; i++)
			{
				if (num%i==0) break;
			}
			if (i == num) n--;
		}
		
		return num;
	}
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
	}

}
